<div class="col-md-4 col-md-offset-4 text-center overbackground ">
  <br>
  <br>
  <div class="card card-container">
            <h2>Iniciar Sesión</h2>
      
      <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name) ) do |f| %>
        <div class="field">
          <%= f.label :email ,"Correo"%><br />
          <%= f.email_field :email, autofocus: true,class:"form-control", autocomplete: "email" %>
        </div>
      
        <div class="field">
          <%= f.label :password ,"Contraseña" %><br />
          <%= f.password_field :password, class:"form-control",autocomplete: "off" %>
        </div>
      
        <% if devise_mapping.rememberable? -%>
          <div class="field">
            <%= f.check_box :remember_me %>
            <%= f.label :remember_me,"Recordarme"%>
          </div>
        <% end -%>
      
        <div class="actions">
          <%= f.submit "Iniciar Sesión", class:"btn btn-default"%>
        </div>
      <% end %>
      
      <%= render "devise/shared/links" %>
  </div>
<br><br></br></br>
</div>