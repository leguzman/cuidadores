   <div class="col-md-4 col-md-offset-4 text-center overbackground ">
  
    <h2>Registrarse</h2>
    
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>
    
      <div class="field">
        <%= f.label :email ,"Correo"%><br />
        <%= f.email_field :email, autofocus: true, class:"form-control", autocomplete: "email" %>
      </div>
    
      <div class="field">
        <%= f.label :password ,"ContraseÃ±a"%>
        <% if @minimum_password_length %>
        <em>(<%= @minimum_password_length %> characters minimum)</em>
        <% end %><br />
        <%= f.password_field :password,  class:"form-control",autocomplete: "off" %>
      </div>
    
      <div class="field">
        <%= f.label :password_confirmation ,"Confirmar contraseÃ±a"%><br />
        <%= f.password_field :password_confirmation, class:"form-control",autocomplete: "off" %>
      </div>
      <br>
      <div class="actions">
        <%= f.submit "Registrarse",class:"btn btn-default" %>
      </div>
    <% end %>
    
    <%= render "devise/shared/links" %>
    <br><br></br></br>
  </div>
  